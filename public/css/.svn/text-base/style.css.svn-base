.stats {
  position: relative;
  display: block;
  padding: 25px 0;
  width: 100%;
  background: rgba(214, 24, 87, 1); 
}

.stats .stat {
  position: relative;
  display: block;
  width: 30%;
  color: #fff;
  text-align: center;
  z-index: 3;
  height: 40px;
}

.stats .stat.age {
  padding-top: 5px;
}

.stats .stat {
  cursor: pointer;
}

.stat span.user-age, .stat small.user-age, .stat.editing i.user-location, .stat span.user-location, .stat.location span.user-location {
  opacity: 1;
  -webkit-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -moz-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -o-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  transition-timing-function: cubic-bezier(.25,.25,0,.95);
}

.stat.editing span.user-age, .stat.editing small.user-age, .stat.editing i, .stat.location.editing span.user-location {
  opacity: 0;
  -webkit-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -moz-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -o-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  transition-timing-function: cubic-bezier(.25,.25,0,.95);
}

.stats .stat:after {
  content: '/';
  position: absolute;
  font-weight: 300;
  color: #fff;
  opacity: 0.3;
  font-size: 26px;
  right: 0;
  top: 50%;
  margin-top: -10px;
}

.stats .stat:last-child:after {
  content: '';
}

.stat.age small.user-age {
  font-weight: 700;
  font-size: 10px;
  display: block;
  text-transform: uppercase;
  margin-bottom: 5px;
}

.stat.age span.user-age {
  font-size: 20px;
}

.stat input.editable {
  margin: 0 10px;
  -webkit-transform: translate3d(0,-120px,0);
  transform: translate3d(0,-120px,0);
  transition: all .5s cubic-bezier(.25,.25,0,.95);
  -webkit-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -moz-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -o-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  transition-timing-function: cubic-bezier(.25,.25,0,.95);
  position: absolute;
  top: 50%;
  margin-top: -20px; 
  border-radius: 22px;
  visibility: hidden;
  opacity: 0;
  padding-right: 25px;
}

.stat.editing input {
  visibility: visible;
  opacity: 1;
  -webkit-transform: translate3d(0,0,0);
  transform: translate3d(0,0,0);
  transition: all .5s cubic-bezier(.25,.25,0,.95);
  -webkit-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -moz-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  -o-transition-timing-function: cubic-bezier(.25,.25,0,.95);
  transition-timing-function: cubic-bezier(.25,.25,0,.95);
}

.stat.status .indicator {
  display: block;
  border-radius: 100%;
  width: 20px;
