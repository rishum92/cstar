  -webkit-appearance: none;
  margin-top: -0px;
}

input[type=range]:focus::-ms-fill-lower {
  background: #a9a9a9;
}

input[type=range]:focus::-ms-fill-upper {
  background: #a9a9a9;
}

input[type="checkbox"] {
  display: none; 
}

input[type="checkbox"] + label {
  color: #3e3e3e;
  font-size: 12px;
  font-weight: 600;
  text-transform: uppercase;
  position: relative; 
  cursor: pointer;
}

input[type="checkbox"] + label b {
  font-weight: bold; 
}

input[type="checkbox"] + label:before {
  content: '';
  display: inline-block;
  width: 16px;
  height: 16px;
  font-family: 'Ionicons';
  padding-top: 1px;
  font-size: 12px;
  text-align: center;
  margin: -3px 12px 0 0;
  vertical-align: middle;
  cursor: pointer; 
}

input[type="checkbox"] + label:before {
  background-color: transparent;
  border: 1px solid #a9a9a9;
}

input[type="checkbox"]:checked + label:before {
  border: 1px solid #a9a9a9;
  content: '\f122';
  background-color: transparent;
  color: #D61857; 
}

input[type="checkbox"] + label:before,
input[type="checkbox"]:checked + label:before {
  transition: background-color 0.4s linear; 
}

.step input[type="radio"] {
  display: none; 
}

.step input[type="radio"] + label {
  color: #3e3e3e;
  font-size: 12px;
  font-weight: 600;
  text-transform: uppercase;
  position: relative;
  cursor: pointer;
  padding: 10px 15px;
  border: 1px solid #eaeaea;
  margin-right: 15px;
  margin-top: 3px;
  -webkit-transition: all 0.3s ease-in-out;
  -moz-transition: all 0.3s ease-in-out;
  -o-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}

.step input[type="radio"] + label:hover, input[type="radio"]:checked + label {
  background: #d61857;
  color: #fff;
  border-color: #d61857;
}

.step input[type="radio"] + label i {
  margin-right: 3px;
}

.step .input-group .form-control[name="location"] {
  width: 100%;
}



