.explore-gallery .photo {
  position: relative;
  width: 25%;
  /*background: #3e3e3e;*/
  padding: 0px;
  /*margin: 6px 0px 6px 6px;*/
  -webkit-transition : all 250ms ease-out;
  -moz-transition : all 250ms ease-out;
  -o-transition : all 250ms ease-out;
  transition : all 250ms ease-out;

}

.explore-gallery .photo:hover {
  cursor:pointer;
  opacity: 0.8;
}

.explore-gallery .photo img {
  width: 100%;
  display: block;
}

.explore-gallery .photo  a:hover img {
  opacity: 0.7;
  -webkit-transition: all 250ms cubic-bezier(0.330, 0.410, 0.785, 0.425);
  -moz-transition: all 250ms cubic-bezier(0.330, 0.410, 0.785, 0.425);
  -o-transition: all 250ms cubic-bezier(0.330, 0.410, 0.785, 0.425);
  transition: all 250ms cubic-bezier(0.330, 0.410, 0.785, 0.425)
}

.explore-gallery .photo .controls {
  position: absolute;
  top: 10px;
  left: 10px;
}

